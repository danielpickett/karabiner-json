{
  "title": "Daniel Pickett - Main Key Mods",
  "rules": [
    {
      "description": "Daniel Pickett - Arrow Key Mod",
      "manipulators": [
        {"type": "basic","from": {"key_code": "caps_lock", "modifiers": {"optional": ["any"]}},"to": [{"key_code": "fn"}]},
        {"type": "basic", "from": {"key_code": "u", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "left_arrow", "modifiers": ["left_command"]}]},
        {"type": "basic", "from": {"key_code": "o", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "right_arrow", "modifiers": ["left_command"]}]},
        {"type": "basic", "from": {"key_code": "i", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "up_arrow"}]},
        {"type": "basic", "from": {"key_code": "j", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "left_arrow"}]},
        {"type": "basic", "from": {"key_code": "k", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "down_arrow"}]},
        {"type": "basic", "from": {"key_code": "l", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "right_arrow"}]},
        {"type": "basic", "from": {"key_code": "home", "modifiers": {"optional": ["any"]}}, "to": [{"key_code": "left_arrow", "modifiers": ["left_command"]}]},
        {"type": "basic", "from": {"key_code": "end", "modifiers": {"optional": ["any"]}}, "to": [{"key_code": "right_arrow", "modifiers": ["left_command"]}]}
      ]
    },
    {
      "description": "Daniel Pickett - Media Keys",
      "manipulators": [
        {"type": "basic", "from": {"key_code": "spacebar", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "play_or_pause"}]}
      ]
    },
    {
      "description": "Daniel Pickett - 40% Test",
      "manipulators": [
        {"type": "basic", "from": {"key_code": "q", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "1", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "w", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "2", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "v", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "3", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "d", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "4", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "f", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "5", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "c", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "6", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "a", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "7", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "s", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "8", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "z", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "9", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "x", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "0", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "e", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "9", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "r", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "0", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "t", "modifiers": {"mandatory": ["fn"]}}, "to": [{"key_code": "9", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "y", "modifiers": {"mandatory": ["fn"]}}, "to": [{"key_code": "0", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "g", "modifiers": {"mandatory": ["fn"]}}, "to": [{"key_code": "open_bracket", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "h", "modifiers": {"mandatory": ["fn"]}}, "to": [{"key_code": "close_bracket", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "b", "modifiers": {"mandatory": ["fn"]}}, "to": [{"key_code": "open_bracket"}]},
        {"type": "basic", "from": {"key_code": "n", "modifiers": {"mandatory": ["fn"]}}, "to": [{"key_code": "close_bracket"}]},
        {"type": "basic", "from": {"key_code": "m", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "hyphen"}]},
        {"type": "basic", "from": {"key_code": "m", "modifiers": {"mandatory": ["fn", "left_shift"], "optional": ["any"]}}, "to": [{"key_code": "hyphen", "modifiers": ["left_shift"]}]},
        {"type": "basic", "from": {"key_code": "comma", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "grave_accent_and_tilde"}]}
      ]
    }
  ]
}
